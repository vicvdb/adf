											}
										},
										{
											"source": {
												"path": "['type']"
											},
											"sink": {
												"name": "type",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['users']"
											},
											"sink": {
												"name": "users",
												"type": "String"
											}
										},
