												"path": "['name']"
											},
											"sink": {
												"name": "name",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['isReadOnly']"
											},
											"sink": {
												"name": "isReadOnly",
												"type": "Boolean"
											}
										},
										{
											"source": {
