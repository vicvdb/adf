{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "Data-Factory-Victor-DV"
		},
		"BdDVictor_connectionString": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « connectionString » de « BdDVictor »"
		},
		"API_REST_PowerBI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.powerbi.com/v1.0/myorg/admin/"
		},
		"API_REST_PowerBI_properties_typeProperties_aadResourceId": {
			"type": "string",
			"defaultValue": "https://analysis.windows.net/powerbi/api"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Data-Factory-Victor-DV_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BdDVictor_connectionString": {
						"value": "[parameters('BdDVictor_connectionString')]"
					},
					"API_REST_PowerBI_properties_typeProperties_url": {
						"value": "[parameters('API_REST_PowerBI_properties_typeProperties_url')]"
					},
					"API_REST_PowerBI_properties_typeProperties_aadResourceId": {
						"value": "[parameters('API_REST_PowerBI_properties_typeProperties_aadResourceId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Data-Factory-Victor-DV_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Data-Factory-Victor-DV_ArmTemplate_0')]"
			]
		}
	]
}